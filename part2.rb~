def hello(name)
	n = "Hello, " + name
		return n
end

def starts_with_consonant? s
	
	if s =~ /\A[aeiouAEIOU]/
		return false
	elsif s=~ /\d[0-9]/
		return false
	elsif s == ""
		return false
	else
		return true
	end		
end

def binary_multiple_of_4? s
	if s[s.length-1]=="0" and s[s.length-2]=="0"
		return true
	else 
		return false
	end
end
